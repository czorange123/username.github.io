<div class="container">
  <div class="header">
    <div class="portrait">
      <img src="/images/34339465.jpg" alt="">
    </div>
    <div class="navbar">
      <ul>
        <li><a href="/">主页</a></li>
        <li><a href="tags">文章</a></li>
        <li><a href="tags">标签</a></li>
        <li><a href="tags">朋友</a></li>
        <li><a href="tags">关于</a></li>
        <li><a href="tags">抓到我</a></li>
      </ul>
    </div>
  </div>
  <div class="post-list">
    <% for(let psg of page.posts.data){ %>
      <div class="post-item" title="<%- psg.title %>">
        <span class="time-m-d"><%- psg.date.format("YYYY-MM-DD") %></span>
        <a href="<%- url_for(psg.path) %>">
          <span><%- psg.title %></span>
        </a>
      </div>
    <% } %>
  </div>
  <nav class="post-navigation">
    <% if(page.prev !== 0){ %>
      <a href="<%- url_for(page.prev_link)%>"><i class="iconfont icon-prev"></i></a>
    <% } %>
    <span class="page-num"><%- page.current%> / <%- page.total -%></span>
    <% if(page.next !== 0){ %>
      <a href="<%- url_for(page.next_link)%>"><i class="iconfont icon-next"></i></a>
    <% } %>
  </nav>
</div>
